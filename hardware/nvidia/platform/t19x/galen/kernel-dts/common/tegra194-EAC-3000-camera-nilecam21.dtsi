	gpio@2200000 {
		camera-control-output-high {
                        status = "okay";
                        gpio-hog;
                        gpios = <CAM3_PWDN 0>;
                        output-high;
                        label = "cam3-pwdn";
                };
		camera-control-output-low {
                        status = "okay";
			gpio-hog;
			gpios = <CAM2_PWDN 0>;
			output-low;
			label = "cam2-pwdn";
		};
	};

